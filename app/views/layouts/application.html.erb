
<!DOCTYPE html>
<html>
<head>
  <title>DiabeticSolution<%= "::#{yield :page_title}" %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
  <body >
   <nav class="navbar navbar-inverse">
    <% if user_signed_in? %>
      <p> Welcome <%= current_user.email %><%= link_to "Sign Out", destroy_user_session_path,  method: :get %> </p>
    <% else %>
      <%= link_to "Sign Up", new_user_registration_path ,class: 'glyphicon glyphicon-search'%>
      <%= link_to "Login", new_user_session_path %>
    <% end %>
  </nav>
   
  <% flash.each do |type, message| %>
    <div class="alert <%= bootstrap_class_for(type) %> alert-dismissable fade in">
      <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
      <%= message %>
    </div>
<% end %>

  <%= yield %>
  
  </body>
</html>
